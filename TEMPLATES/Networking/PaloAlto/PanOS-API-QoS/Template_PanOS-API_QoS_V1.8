zabbix_export:
  version: '7.0'
  template_groups:
    - uuid: 7df96b18c230490a9a0a9e2307226338
      name: Templates

  templates:
    - uuid: 3f0c2a1b9d3e4f12a8b9c0d1e2f3a4b5
      template: 'Template - PanOS-API - QoS'
      name: 'Template - PanOS-API - QoS'
      description: |
        Collect QoS per-class throughput using PAN-OS API (API key authentication).

        Deployment macros to set on the host or host group:
        - {$API_KEY}                 PAN-OS API key
        - {$QOS_INTERFACES}          Pipe-separated interface list for discovery (example: ethernet1/2|ethernet1/3)

        Per-class static threshold macros (bps):
        - {$QOS_THR_WARN_CLASSN}     Static warning threshold for class N (default: 10000000000)
        - {$QOS_THR_HIGH_CLASSN}     Static high threshold for class N (default: 10000000000)
        - {$QOS_THR_DISASTER_CLASSN} Static disaster threshold for class N (default: 10000000000)

        Per-class dynamic alerting macros (bps and time windows):
        - {$QOS_MIN_BASELINE_CLASSN}    Minimum baseline for class N (default: 10000000)
        - {$QOS_SPIKE_SHORT_CLASSN}     Short window for class N (default: 10m)
        - {$QOS_SPIKE_LONG_CLASSN}      Long window for class N (default: 1h)
        - {$QOS_SPIKE_WARN_CLASSN}      Spike warning factor for class N (default: 2)
        - {$QOS_SPIKE_HIGH_CLASSN}      Spike high factor for class N (default: 3)
        - {$QOS_SPIKE_DISASTER_CLASSN}  Spike disaster factor for class N (default: 5)

        Per-class trend alerting macros:
        - {$QOS_TREND_SHORT_CLASSN}     Short trend window for class N (default: 24h)
        - {$QOS_TREND_LONG_CLASSN}      Long trend window for class N (default: 7d)
        - {$QOS_TREND_FACTOR_CLASSN}    Trend factor for class N (default: 1.5)

        No data macro:
        - {$QOS_NODATA}              No data window for master item (default: 10m)

        Notes:
        - Master item retrieves throughput in kbps. Dependent items convert it to bps.
        - Each QoS class has independent threshold and dynamic trigger parameters.
        - Both static threshold triggers and dynamic (spike/trend) triggers are enabled.
        - Spike triggers compare avg(short) vs avg(long) and are mutually exclusive by ranges.
        - Trend trigger compares avg(trend_short) vs avg(trend_long) and is informational.
      groups:
        - name: Templates
      vendor:
        name: N1k0droid
        version: '1.8.0'

      discovery_rules:
        - uuid: 8c1d0e2f7a3b4c56b8d9a0e1f2a3b4c5
          name: 'QoS interface discovery'
          type: SCRIPT
          key: panos.qos.if.discovery
          delay: 1h
          params: |
            // Zabbix JavaScript
            var ifs = "{$QOS_INTERFACES}";
            if (!ifs) {
              return JSON.stringify({data: []});
            }
            var arr = ifs.split("|").map(function(s){ return s.trim(); }).filter(function(s){ return s.length > 0; });
            var data = arr.map(function(i){ return {"{#IFNAME}": i}; });
            return JSON.stringify({data: data});
          description: 'Discover QoS interfaces from {$QOS_INTERFACES}'
          item_prototypes:
            - uuid: 2b3c4d5e6f704b23a8c9d0e1f2a3b4c6
              name: 'QoS class 1 throughput [{#IFNAME}]'
              type: DEPENDENT
              key: 'panos.qos.class1.bps[{#IFNAME}]'
              delay: '0'
              history: 90d
              value_type: FLOAT
              units: bps
              description: 'QoS class 1 throughput converted to bps.'
              preprocessing:
                - type: REGEX
                  parameters:
                    - 'Class 1\\s+([0-9]+)\\s+kbps'
                    - '\\1'
                - type: MULTIPLIER
                  parameters:
                    - '1000'
              master_item:
                key: 'panos.qos.raw[{#IFNAME}]'
              trigger_prototypes:
                - uuid: c01b2c3d4e5f4a12b8c9d0e1f2a3b413
                  expression: 'avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_SHORT_CLASS1})>{$QOS_SPIKE_DISASTER_CLASS1}*avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_LONG_CLASS1}) and avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_LONG_CLASS1})>{$QOS_MIN_BASELINE_CLASS1}'
                  name: 'QoS {#IFNAME} class 1 throughput spike disaster'
                  priority: DISASTER
                  description: 'Short window average is above the disaster spike factor compared to the long window average.'
                - uuid: b01b2c3d4e5f4a12b8c9d0e1f2a3b412
                  expression: 'avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_SHORT_CLASS1})>{$QOS_SPIKE_HIGH_CLASS1}*avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_LONG_CLASS1}) and avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_SHORT_CLASS1})<={$QOS_SPIKE_DISASTER_CLASS1}*avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_LONG_CLASS1}) and avg(/Template - PanOS-API - QoS/panos.qos.class1.bps[{#IFNAME}],{$QOS_SPIKE_LONG_CLASS1})>{$QOS_MIN_BASELINE_CLASS1}'
                  name: 'QoS {#IFNAME} class 1 throughput spike high'
                  priority: HIGH
                  description: 'Short window average is above the high spike factor compared to the long window average.'
                - uuid: a01b2c3d4e5f4a12b8c9d0e1f2a3b411
